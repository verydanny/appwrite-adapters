{
    "name": "@gravlabs/appwrite-hono-adapter-node",
    "version": "0.5.0",
    "type": "module",
    "main": "lib/main.js",
    "module": "lib/main.js",
    "scripts": {
        "build": "tsup",
        "postbuild": "bun run build:types",
        "build:types": "tsc -b",
        "postbuild:types": "publint",
        "build:watch": "tsup --watch",
        "lint": "biome lint ./src",
        "lint:fix": "biome lint --write ./src",
        "format": "biome format --write ./src",
        "format:fix": "biome format --write ./src",
        "start": "nodemon --env-file=.env ./server/server.cjs"
    },
    "exports": {
        ".": {
            "types": "./lib/main.d.ts",
            "require": "./lib/main.cjs",
            "import": "./lib/main.js"
        },
        "./serveStatic": {
            "types": "./lib/serveStatic.d.ts",
            "require": "./lib/serveStatic.cjs",
            "import": "./lib/serveStatic.js"
        },
        "./types": {
            "types": "./lib/types.d.ts"
        }
    },
    "typesVersions": {
        "*": {
            ".": [
                "./lib/main.d.ts"
            ],
            "serveStatic": [
                "./lib/serveStatic.d.ts"
            ],
            "types": [
                "./lib/types.d.ts"
            ]
        }
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org",
        "access": "public"
    },
    "homepage": "https://github.com/verydanny/appwrite-adapters/tree/main/packages/adapter-node",
    "repository": {
        "type": "git",
        "url": "https://github.com/verydanny/appwrite-adapters.git"
    },
    "files": [
        "lib",
        "!lib/*.d.ts.map"
    ],
    "devDependencies": {
        "@mobily/ts-belt": "^3.13.1",
        "@tsconfig/node21": "^21.0.3",
        "@types/node": "^20.14.10",
        "@biomejs/biome": "^1.8.3",
        "nodemon": "^3.1.4",
        "publint": "^0.2.8",
        "tsup": "^8.1.0",
        "typescript": "^5.5.3"
    },
    "peerDependencies": {
        "hono": "^4.4.12"
    }
}
